class Student:
    schoolname = "SUDHEER"
    def __init__(self,name,marks):
        self.name=name
        self.marks=marks

    def info(self):
        print("your name is",self.name)
        print("your marks are",self.marks)
....class method...
    def getschoolname(cls):
        print("school name",cls.schoolname)
        
s1 = Student("sudheer",100)
s1.info()
s1.getschoolname()
